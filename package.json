{
	"name": "phosart-stash-template",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "(svelte-kit sync || echo '') && husky",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"edit": "cross-env ORIGIN=http://127.0.0.1:3000 BODY_SIZE_LIMIT=Infinity phosart-devtool",
		"editpreview": "concurrently -i \"pnpm run dev --port 15483\" \"dotenvx run -- cross-env PORT=15482 PREVIEWPORT=15483 ORIGIN=http://127.0.0.1:15482 BODY_SIZE_LIMIT=Infinity phosart-devtool\""
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "^1.51.4",
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@sveltejs/adapter-vercel": "^6.2.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^22",
		"concurrently": "^9.2.1",
		"cross-env": "^10.1.0",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"phosart-devtool": "github:phosphoriteart/phosart-devtools#stable",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"sharp": "^0.34.5",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vite": "^7.2.6"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^7.1.0",
		"array-shuffle": "^4.0.0",
		"color": "^5.0.3",
		"es-toolkit": "^1.43.0",
		"fuzzysort": "^3.1.0",
		"phosart-common": "github:phosphoriteart/phosart-common#stable"
	}
}
